        <activity
            android:name=".AdicionarItemPorTipo"
            android:label="Escolher item" />
        <activity
            android:name=".AdicionarItem"
            android:label="Escolher tipo de item" />
        <activity android:name=".ListaPedidos">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>